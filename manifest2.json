{
    "manifest_version": 3,
    "name": "Remover Images",
    "description": "Remover todas as imagens de um site e substituir por suas descrições.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Remover Imagens"
    }
}